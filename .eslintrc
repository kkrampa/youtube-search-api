{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "global-require": 0
  },
  "globals": {
    "localStorage": true,
    "window": true,
    "document": true
  }
}
